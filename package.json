{
  "name": "cached-fetcher",
  "version": "0.0.1-dev",
  "description": "Promise based cached data fethcer",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/abihf/cached-fetcher",
  "author": "Abi Hafshin <abi@hafs.in>",
  "license": "MIT",
  "private": false,
  "files": [
    "README.md",
    "LICENSE.md",
    "src/",
    "lib/",
    "esm/"
  ],
  "scripts": {
    "lint": "tslint -p tsconfig.json",
    "build": "tsc",
    "build:esm": "tsc -p tsconfig.esnext.json",
    "test": "jest",
    "test:ci": "jest --coverage --ci --testResultsProcessor=\"jest-junit\""
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^22.1.4",
    "@types/node": "^9.4.6",
    "jest": "^22.4.2",
    "jest-junit": "^3.6.0",
    "ts-jest": "^22.4.0",
    "tslint": "^5.9.1",
    "tslint-language-service": "^0.9.8",
    "typescript": "^2.7.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
